                }
            if (n == 1)
            {
                Console.WriteLine("Введите значание а");
                int a = Convert.ToInt32(Console.ReadLine());
                Console.WriteLine("Введите значание b");
                int b = Convert.ToInt32(Console.ReadLine());
                Oper<int> op = new Oper<int>(a, b);
                Console.Write("Сумма: ");
                op.Summ(a, b);
                Console.Write("Разность: ");
                op.Diff(a, b);
                Console.Write("Произведение: ");
                op.Time(a, b);
                Console.Write("Частное: ");
                op.Quot(a, b);
            }
            if (n == 2)
            {
                Console.WriteLine("Введите значание а");
                double a = Convert.ToDouble(Console.ReadLine());
                Console.WriteLine("Введите значание b");
                double b = Convert.ToDouble(Console.ReadLine());
                Oper<double> op = new Oper<double>(a, b);
                Console.Write("Сумма: ");
                op.Summ(a, b);
                Console.Write("Разность: ");
                op.Diff(a, b);
                Console.Write("Произведение: ");
