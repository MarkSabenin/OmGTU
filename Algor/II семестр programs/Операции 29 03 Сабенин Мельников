namespace ConsoleApplication37
{
    class Program
    {
        public class Oper<T> where T : struct
        {
            T a;
            T b;
            public Oper(T a, T b)
            {
                this.a = a;
                this.b = b;
            }
            public T A { get; set; }
            public T B { get; set; }
            public void Summ(T a, T b)
            {
                dynamic a1 = a;
                dynamic b1 = b;
                Console.WriteLine( b1+a1);
            }
            public void Diff(T a, T b)
            {
                dynamic a1 = a;
                dynamic b1 = b;
                Console.WriteLine(a1 - b1);
            }
            public void Time(T a, T b)
            {
                dynamic a1 = a;
                dynamic b1 = b;
                Console.WriteLine(a1 * b1);
            }
            public void Quot(T a, T b)
            {
                dynamic a1 = a;
                dynamic b1 = b;
                Console.WriteLine(a1 / b1);
            }

        }
        static void Main(string[] args)
        {
            Console.WriteLine("Выберите тип числовых данных: \n1 - целые числa\n2 - действительные");
