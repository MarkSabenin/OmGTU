using System;
using System.Linq;
class HelloWorld {
  static void Main() {
     int n=Convert.ToInt32(Console.ReadLine());
      int []a =new int [n];
      int a1=0, a2=0;
      int c=0;
      bool d=true;
      for (int i=0; i<n; i++) {
      a[i]=Convert.ToInt32(Console.ReadLine());
      if (i+1%2==0)
      {
      if (a[i]-9<0) {if (a[i]==5) Console.WriteLine(d); else {d = false; Console.WriteLine(d);}}
      else
      while (a[i]-9>0)
      {
      a2=a[i]/10;
      a1=a[i]-a2*10;
      if (a1!=5) d=false;
      a[i]=a[i]/10;
      }
      Console.WriteLine(d);
      }
      
  }
}
}
