      {
          if (g[pos] != 0)
          {
              g[pos] = 0;
              pos+=k;
              if (pos+k > m+n) pos=pos-n-m;
          }
          else 
          {
              while (g[pos] == 0) pos+=1;
              g[pos] = 0;
              pos+=k;
              if (pos+k > m+n) pos=pos-n-m;
          }
          int c = CountOfLife(g);
         
         
          
      }
      
      if (g[0] = 0)
              {
                  if (n1!=c-m1) ;
                  if ()
              }
      
      
    Console.WriteLine();
  }
  static int CountOfLife(int[] g)
  {
      int c = 0;
      foreach (int i in g)
      if (i != 0) c++;
      return c;
  }
